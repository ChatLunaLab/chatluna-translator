{
  "name": "koishi-plugin-chatluna-translator",
  "description": "Translation services powered by the large language model in chatluna",
  "version": "0.0.4",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "dist",
    "resources"
  ],
  "license": "AGPL-3.0",
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "service",
    "chatgpt",
    "chatluna",
    "preset",
    "translator"
  ],
  "author": "dingyi222666 <dingyi222666@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChatLunaLab/chatluna-translator.git",
    "directory": "."
  },
  "bugs": {
    "url": "https://github.com/ChatLunaLab/chatluna-translator/issues"
  },
  "homepage": "https://github.com/ChatLunaLab/chatluna-translator#readme",
  "dependencies": {
    "@koishijs/translator": "^1.1.1",
    "@langchain/core": "^0.2.31"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.1-alpha.1",
    "@typescript-eslint/parser": "^6.21.1-alpha.1",
    "atsc": "^1.2.2",
    "esbuild": "^0.23.1",
    "esbuild-register": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "koishi": "^4.17.12",
    "yakumo": "^1.0.0-beta.18",
    "yakumo-esbuild": "^1.0.0-beta.6",
    "yakumo-mocha": "^1.0.0-beta.2",
    "yakumo-tsc": "^1.0.0-beta.4"
  },
  "peerDependencies": {
    "koishi": "^4.17.9",
    "koishi-plugin-chatluna": "^1.0.0-beta.76"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "yarn yakumo build",
    "bump": "yarn yakumo version",
    "dep": "yarn yakumo upgrade",
    "pub": "yarn yakumo publish",
    "lint": "yarn eslint src --ext=ts",
    "lint-fix": "yarn eslint src --ext=ts --fix"
  },
  "koishi": {
    "description": {
      "zh": "ChatLuna 中的大语言模型驱动提供的翻译服务"
    },
    "service": {
      "required": [
        "chatluna"
      ],
      "implements": [
        "translator"
      ]
    }
  }
}
